{
  "App": {
    "CorsOrigins": ""
  },
  "ConnectionStrings": {
    "Default": "mongodb://127.0.0.1:27017/EoaServer?authSource=admin&retryWrites=false"
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "AuthServer": {
    "Authority": "http://127.0.0.1:8080",
    "RequireHttpsMetadata": "false",
    "SwaggerClientId": "EoaServer_App"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "gaMgs0gtsqpYZUTn"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:7788"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "retainedFileCountLimit": 10
        }
      }
    ]
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "EoaServer"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "127.0.0.1",
        "Port": "5672"
      }
    },
    "EventBus": {
      "ClientName": "API",
      "ExchangeName": "EoaServer"
    }
  },
  "EncryptionInfo": {
    "Key": "12345678901234567890123456789012"
  },
  "Settings": {
    "Abp.Account.IsSelfRegistrationEnabled": false
  },
  "GraphQL": {
    "Configuration": "http://localhost:8101/graphql"
  },
  "Orleans": {
    "ClusterId": "EoaServerSiloCluster",
    "ServiceId": "EoaServerOrleansBasicService",
    "MongoDBClient": "mongodb://localhost:27017/?maxPoolSize=555",
    "DataBase": "EoaServerOrleansDB"
  }
}
