{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "App":{
    "SelfUrl":"https://localhost:44317",
    "ClientUrl":"http://localhost:4200",
    "CorsOrigins":"https://*.CAServer.com,http://localhost:4200,https://localhost:44340,https://localhost:44362",
    "RedirectAllowedUrls":"http://localhost:4200,https://localhost:44395,https://localhost:44340,https://localhost:44364"
  },
  "Kestrel":{
    "EndPoints":{
      "Http":{
        "Url":"http://*:8001/"
      }
    }
  },
  "ElasticUris":{
    "Uris":[
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting":{
    "NumberOfShards":5,
    "NumberOfReplicas":1,
    "IndexPrefix":"CAServer"
  },
  "AuthServer":{
    "IssuerUri":"http://127.0.0.1:8001"
  },
  "ConnectionStrings":{
    "Default": "mongodb://127.0.0.1:27017/EoaServer?retryWrites=false"
  },
  "Redis":{
    "Configuration": "127.0.0.1"
  },
  "StringEncryption":{
    "DefaultPassPhrase":"gaMgs0gtsqpYZUTn"
  },
  "GraphQL":{
    "Url":""
  },
  "RabbitMQ":{
    "Connections":{
      "Default":{
        "HostName": "127.0.0.1",
        "Port":"5672"
      }
    },
    "EventBus":{
      "ClientName":"Eoa_Auth",
      "ExchangeName":"EoaServer"
    }
  },
  "TimeRange":1440,
  "ExpirationHour": 48,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Properties": {
      "Application": "EoaAuthServer.Service",
      "Environment": "Testnet"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  }
}
